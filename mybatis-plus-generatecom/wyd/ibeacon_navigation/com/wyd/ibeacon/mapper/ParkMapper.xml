<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wyd.ibeacon.mapper.ParkMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.wyd.ibeacon.model.po.Park">
        <id column="id" property="id" />
        <result column="park_name" property="parkName" />
        <result column="detail" property="detail" />
        <result column="lamp_count" property="lampCount" />
        <result column="extend_field" property="extendField" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, park_name, detail, lamp_count, extend_field, update_time
    </sql>

</mapper>
